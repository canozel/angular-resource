<h1>Raffle</h1>
<div ng-controller = "RaffleCtrl">
  <form ng-submit = "addEntry()">
    <input type = "text" ng-model = "newEntry.name" >
    <input type = "submit" value="Ekle">
  </form>
  	{{newEntry.name}}
  <ul>
  	<li ng-repeat = "entry in entries.entries" >
  		{{entry.name}}   
  	</li>
  </ul>
</div>